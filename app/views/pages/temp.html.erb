<%= form_for(resource, :as => resource_name, :url => registration_path(resource_name)) do |f| %>

<div class="middle">
    
<%= devise_error_messages! %>
<%= render 'devise/shared/left_column' %>
    
<div class="con_cen">
<div class="choices">
  
  <h4>Sign up for free</h4><br />
  
   <%= f.label :username %><br />
   <%= f.text_field :username %><br /><br />

   <%= f.label :email %><br />
   <%= f.email_field :email %><br /><br />

   <%= f.label :password %><br />
   <%= f.password_field :password %><br /><br />

   <%= f.label :password_confirmation, "Password Confirm" %><br />
   <%= f.password_field :password_confirmation %><br /><br />
   
  <div class="actions">
    <%= f.submit "Sign up" %>
  </div>  
   By clicking Sign up, you agree to the <%= link_to "Terms of Use", legal_path, :rel => "facebox[.facebig]" %><br />
   <%= render :partial => "devise/shared/links" %>
 </div>

<%= link_to "Sign in with Facebook", user_omniauth_authorize_path(:facebook) %><br />
<%= link_to "Sign in with Google", user_omniauth_authorize_path(:google) %><br />



</div>
    <%= render 'devise/shared/right_column' %>
</div>

<% end %>

     <% end %>
    </ul>
		</td>
		</div> 
		
		<td>
		<div class="quiz_type">
		correct answer
		<br /><br />
		<ul>
		<% @interactions.each do |l| %>
		<%= l['correct_answer'] %><br /><br />
		<% end %>
		</ul>
		</td>
		</div>
		
		<td>
		<div class="correct">
		your answer
		<br /><br />
		<ul>
		<% @interactions.each do |l| %>
		<%= l['student_answer'] %><br /><br />
		<% end %>
		</ul>
		</td>
		</div>
		
		<td>
		<div class="you_answered">
		date
		<br /><br />
		<ul>
		<% @interactions.each do |l| %>
		<%= l['date'] %><br /><br />
		<% end %>
		</ul>
		</td>
		</div>
		
		<td>
		<div class="latency">
		
		<br /><br />
		<ul>
		
		</ul>

<div class="row">
      <div class="span4" style="background:#f5f5f5">
      <h3><u>Basic Info</u></h3>
      <h4>email</h4>
      <%=current_user.email %><br />
      <h4>account created</h4>
      <%=current_user.created_at %>
      </div>

      <div class="span4" style="background:#0E2A21">
      <h3 style ="color:#f5f5f5"><u>Puzzles Completed</u></h3>
      <% @titles.each do |t| %>
      <div style = "color:#f5f5f5;"><%= t %></div><br/>
      <% end %>
      </div>

      <div class="span4" style="background:#102C31">
      <h3 style = "color:#f5f5f5;"><u>Puzzles Remaining</u></h3>
      <% @incomplete_puzzles.each do |t| %>
      <div style = "color:#f5f5f5;"><%= t %></div><br />
      <% end %>
      </div>

      <div class="span4" style="background:#252525"></div>

      <div class="span4" style="background:#D3D8E8">
      <h3><u>Quizzes Completed</u></h3>
      <% @atitles.each do |t| %>
      <%= t %><br/>
      <% end %>
      </div>

      <div class="span4" style="background:#0C1E1D">
      <h3 style = "color:#f5f5f5;"><u>Quizzes Remaining</u></h3>
      <% @incomplete_assessments.each do |t| %>
      <div style = "color:#f5f5f5;"><%= t %></div><br />
      <% end %>
      </div>
</div>
</div>
